#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/mnt/c/Users/hp/Downloads/telegram bot payment backend/verifier/custom-cbe-verifier/node_modules/.pnpm/ts-jest@29.4.6_@babel+core@_bd9ef016b116f97eb26b9aeed4f15d7f/node_modules/ts-jest/node_modules:/mnt/c/Users/hp/Downloads/telegram bot payment backend/verifier/custom-cbe-verifier/node_modules/.pnpm/ts-jest@29.4.6_@babel+core@_bd9ef016b116f97eb26b9aeed4f15d7f/node_modules:/mnt/c/Users/hp/Downloads/telegram bot payment backend/verifier/custom-cbe-verifier/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/mnt/c/Users/hp/Downloads/telegram bot payment backend/verifier/custom-cbe-verifier/node_modules/.pnpm/ts-jest@29.4.6_@babel+core@_bd9ef016b116f97eb26b9aeed4f15d7f/node_modules/ts-jest/node_modules:/mnt/c/Users/hp/Downloads/telegram bot payment backend/verifier/custom-cbe-verifier/node_modules/.pnpm/ts-jest@29.4.6_@babel+core@_bd9ef016b116f97eb26b9aeed4f15d7f/node_modules:/mnt/c/Users/hp/Downloads/telegram bot payment backend/verifier/custom-cbe-verifier/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../ts-jest/cli.js" "$@"
else
  exec node  "$basedir/../ts-jest/cli.js" "$@"
fi
